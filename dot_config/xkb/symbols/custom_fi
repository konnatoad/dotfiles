default partial alphanumeric_keys
xkb_symbols "custom" {
    include "fi"

    key <AC10> {
        type= "FOUR_LEVEL",
        symbols[Group1]= [ comma, semicolon, less, NoSymbol ]
    };

    key <AC11> {
        type= "FOUR_LEVEL",
        symbols[Group1]= [ period, colon, bar, NoSymbol ]
    };

    key <AB10> {
        type= "FOUR_LEVEL",
        symbols[Group1]= [ minus, underscore, greater, NoSymbol ]
    };
};
